<div class="user-card">
    <h2><%- name %></h2>
    <p>Email: <%- email %></p>
    <% if (isActive) { %>
    <span class="status active">Active</span>
    <% } else { %>
    <span class="status inactive">Inactive</span>
    <% } %>
    <div class="tags">
        <% tags.forEach(tag => { %>
        <span class="tag"><%- tag %></span>
        <% }); %>
    </div>
</div>
